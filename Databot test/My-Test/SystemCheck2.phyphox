<phyphox xmlns="http://phyphox.org/xml" xmlns:editor="http://phyphox.org/editor/xml" version="1.7" editor:version="1.0" locale="en">
    <title>databot 2.0 System Check</title>
    <category>databot 2.0 Sensors</category>
    <icon format="base64"></icon>
    <color>White</color>
    <description>Check your power level and test lights and sound capabilities with this quick system test.</description>
    <link label="databot Sensors - Technical Details" highlight="true">https://databot.us.com/geek</link>
    <data-containers>
        <container size="0" static="false">m</container>
		<container size="1" static="false">B</container>
		<container size="1" static="false">E</container>
		<container size="1" static="false">Exp_Tx</container>
    </data-containers>
    <input>
        <bluetooth id="DB_" name="DB_" uuid="0000ffe0-0000-1000-8000-00805f9b34fb" mode="notification" rate="1" mtu="220" subscribeOnStart="true">
            <config char="0000ffe2-0000-1000-8000-00805f9b34fb" conversion="string">{"refresh":1000,"decimal":2,"timeFactor":1000,"timeDec":2,"accl":false,"gyro":false,"magneto":false,"IMUtemp":false,"Etemp1":false,"Etemp2":false,"pressure":false,"alti":false,"ambLight":false,"rgbLight":false,"UV":false,"co2":false,"voc":false,"hum":false,"humTemp":false,"Sdist":false,"Ldist":false,"noise":false,"gesture":false,"sysCheck":true,"altCalib":false,"led1":{"state":false,"R":255,"Y":0,"B":0},"led2":{"state":false,"R":0,"Y":255,"B":0},"led3":{"state":false,"R":0,"Y":0,"B":255}}
			</config>
			<output char="0000ffe1-0000-1000-8000-00805f9b34fb" conversion="formattedString" separator=";" label="m">m</output>
            <output char="0000ffe1-0000-1000-8000-00805f9b34fb" conversion="formattedString" separator=";" label="B">B</output>
			<output char="0000ffe1-0000-1000-8000-00805f9b34fb" conversion="formattedString" separator=";" label="E">E</output>
        </bluetooth>
    </input>
	<output>
		<bluetooth id="DB_" name="DB_" uuid="0000ffe0-0000-1000-8000-00805f9b34fb" mode="notification" rate="1" mtu="220" subscribeOnStart="true">			
			<input char="0000ffe2-0000-1000-8000-00805f9b34fb" conversion="string">Exp_Tx</input>
		</bluetooth>
    </output>
    <analysis sleep="0"  onUserInput="false">
	<append>
		<input type="value">1</input>
		<output>Exp_Tx</output>
	</append>	
    </analysis>
    <views>
        <view label="Main">

        <info color="white" label="Press the Start Button to run a test on your databot 2.0 lights and sound.  You should see a sequence of red and blue lights and hear a little tune." bold="false" italic="false"  size="1.3" />  
        <separator color="black" height="0.2" />
        <separator color="white" height="0.3" />
		<separator color="black" height="0.4" />
        <info color="white" label="Your databot 2.0 Identifier information is:" bold="false" italic="false"  size="1.3" />
        
        <separator color="black" height="0.6" />

            
        <value label="" size="2" precision="0" scientific="false" unit="" color="ffffff">
            <input>E</input>
        </value>

        <separator color="black" height="0.6" />

        <info color="ffffff" label="When connecting to your databot by WiFi you will be looking for a device address (SSID) with the above ID included.  Once connected, use the databot IP address (192.168.1.160) to access your control menu." bold="false" italic="false"  size="1.3" />
        
        <separator color="black" height="0.6" />
        

        <info color="ffffff" label="Ready?  Go Forth and Explore!" bold="false" italic="false"  size="1.3" />
        
        </view>
    </views>
    <export>
        <set name="databot">
            <data name="Time (S)">m</data>
            <data name="Battery">B</data>
        </set>
    </export>
</phyphox>
